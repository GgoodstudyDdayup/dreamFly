/* 列表开始 */

.listAll {
}
.null{
    height: 20rpx;
    background: #f3f3f3;
}
.listTop {
    box-sizing: border-box;
    padding: 16rpx;
    display: flex;
    justify-content: space-between;
    background: #fff;
}

.listTopL {
    display: flex;
}

.listTopL image {
    width: 90rpx;
    height: 90rpx;
    border-radius: 50%;
    display: inline-block;
}

.userInfo {
    margin-left: 10rpx;
    line-height: 50rpx;
}

.name {
    color: #000;
    font-size: 34rpx;
    width: 400rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.date {
    font-size: 28rpx;
    color: #9c9c9c;
}

.logo {
    width: 90rpx;
    height: 90rpx;
    border-radius: 50%;
}

.picImg {
    display: flex;
    justify-content: space-between;
}

.picImg image {
    height: 390rpx;
    width: 100%;
}

.video {
    height: 390rpx;
    width: 100%;
}

.video image {
    width: 100%;
    height: 100%;
}

.video video {
    width: 100%;
}

.picImg image:nth-child(2) {
    margin-left: 16rpx;
}

.listTitle {
    font-size: 36rpx;
    font-weight: 700;
    box-sizing: border-box;
    padding: 16rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    background: #fff;
}

.dianzan {
    display: flex;
    align-items: center;
}

.bottomMenu {
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-size: 24rpx;
    border-bottom: 1rpx solid #f1f1f1;
    padding-bottom: 16rpx;
    padding-left: 16rpx;
    padding-right: 16rpx;
    background: #fff;
}

.bottomMenu-item {
    display: flex;
    align-content: center;
    margin-right: 10rpx;
}

.bottomMenu-content {
    display: flex;
    align-items: center;
}

.xin {
    width: 32rpx;
    height: 32rpx;
}

.flaR {
    float: right;
}

.bottomMenu .mar-r-10 {
    margin-right: 10rpx;
}

.btn {
    padding: 0;
    margin: 0;
    display: inline-block;
    line-height: 0;
    border-radius: 0rpx;
    background: #fff;
}

button::after {
    border: none;
}

.msgList {
    box-sizing: border-box;
    padding: 30rpx;
    font-size: 24rpx;
    color: #7d7d7d;
    background: #fff;
}
