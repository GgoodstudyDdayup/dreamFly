/* 商品列表 */
.goods {
  margin: 0 5%;
}
.goods-item {
  height: 150rpx;
  display: flex;
  padding: 20rpx 0;
  border-bottom: 1rpx solid #eee;
}
.item-img,
.item-img image {
  width: 150rpx;
  height: 150rpx;
}
.item-info {
  box-sizing: border-box;
  padding-left: 15rpx;
  position: relative;
  width: 100%;
  min-height: 150rpx;
}
.info-title {
  display: block;
  color: #473e3e;
  font-size: 30rpx;
  font-weight: 600;
}
.info-money {
  position: absolute;
  bottom: 0;
}
.goodsMoney {
  color: #888888;
  font-size: 32rpx;
}
.goodsMoney text {
  font-weight: 600;
  font-size:40rpx;
}
.price-describe,
.price-former {
  color: #888888;
  font-size: 24rpx;  
}
.price-former {
  float: right;
  padding-top: 20rpx;
}
.info-icon,
.info-icon image {
  width: 70rpx;
  height: 60rpx;
}
.info-icon {
  position: absolute;
  bottom: 0;
  right: 0;
}



