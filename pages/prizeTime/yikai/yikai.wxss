/* pages/prizeTime/yikai/yikai.wxss */
.prizeTime {
  box-sizing: border-box;
  padding: 32rpx;
  display: flex;
  justify-content: space-between;
  align-items:center;
}

.prizeTime view:nth-child(1) {
  font-size: 32rpx;
}

.prizeTime view:nth-child(2) {
  font-size: 30rpx;
  color: #6d6d6d;
  background: #f3f3f3;
  box-sizing: border-box;
  padding: 10rpx;
  border-radius: 6rpx;
}

.prizePic{
  width: 100%;
  height: 300rpx;
  box-sizing: border-box;
  padding: 0 24rpx 24rpx 24rpx;
  margin-top: 24rpx;
}

.prizePic image{
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}

.fontSize{
font-size:28rpx;
font-weight:600;
}
.prizeTitle{
  width: 100%;
  box-sizing: border-box;
