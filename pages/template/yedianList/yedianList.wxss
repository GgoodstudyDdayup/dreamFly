/* 列表开始 */

.window {
    width: 100%;
    background: #fefefe;
    margin-top: 20rpx;
    padding-bottom: 20rpx;
}

.windone {
    width: 100%;
    padding-bottom: 20rpx;
    background: #fefefe;
    margin-top: 20rpx;
}

.shopList {
    margin: 0 64rpx 64rpx 64rpx;
    box-shadow: 0rpx 5rpx 30rpx rgb(146, 146, 146);
    position: relative;
    border-radius: 10rpx;
}

.shopList1 {
    margin: 0 64rpx 64rpx 64rpx;
    box-shadow: 0rpx 5rpx 30rpx rgb(146, 146, 146);
    position: relative;
    border-radius: 10rpx;
}

.ones {
    width: 90%;
    height: 100rpx;
    margin: 0 auto;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.shopListBottomL {
    display: flex;
    align-items: center;
}

.shopListPart {
    width: 110%;
    height: 300rpx;
    position: absolute;
    top: 100rpx;
    left: -5%;
    box-shadow: 0rpx 6rpx 16rpx rgb(175, 175, 175);
    border-radius: 10rpx;
    overflow: hidden;
}

.shopListPart1 {
    width: 110%;
    height: 300rpx;
    position: absolute;
    top: 60rpx;
    left: -5%;
    box-shadow: 0rpx 6rpx 16rpx rgb(175, 175, 175);
    border-radius: 10rpx;
    overflow: hidden;
}

.shopListPart image, .shopListPart1 image {
    width: 100%;
    height: 100%;
}

.shopListTitle {
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    background: rgba(0, 0, 0, 0.4);
    color: #fff;
    font-size: 24rpx;
    font-weight: 700;
    box-sizing: border-box;
    padding: 24rpx 20rpx;
    display: flex;
    justify-content: space-between;
}

/* .shopListTitle :nth-child(1) {
  width: 70%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
} */

.shopListTop {
    box-sizing: border-box;
    position: absolute;
    width: 100%;
    top: 0;
    left: 0;
    padding: 10rpx 30rpx;
    display: flex;
    justify-content: space-between;
}

.shopListTop1 {
    box-sizing: border-box;
    position: absolute;
    width: 100%;
    /* height: 60rpx; */
    top: 70rpx;
    left: 0;
    /* padding: 10rpx 30rpx; */
    display: flex;
    justify-content: space-between;
    /* background: #5bc9d4; */
    z-index: 1;
}

.shopListTopL, .shopListTopL1 {
    color: #060206;
    text-align: center;
}

.year {
    font-size: 24rpx;
    letter-spacing: 40rpx;
}

.line {
    height: 1rpx;
    background: #969496;
    width: 85%;
}

.day {
    font-size: 38rpx;
    font-weight: 700;
    padding-right: 15%;
}

.day1 {
    font-size: 28rpx;
}

.shopListTopR {
    font-size: 32rpx;
    line-height: 80rpx;
}

.shopListTopR1 {
    font-size: 32rpx;
}

.shopListTopR image, .shopListTopR1 image {
    width: 40rpx;
    height: 40rpx;
    margin-right: 10rpx;
    vertical-align: middle;
}

.shopListBottom {
    padding: 30rpx 20rpx;
    box-sizing: border-box;
    width: 100%;
    display: flex;
    justify-content: space-between;
    /* background: #5bc9d4; */
}

.shopListBottom1 {
    padding: 0 20rpx 20rpx 20rpx;
    box-sizing: border-box;
    width: 100%;
    display: flex;
    justify-content: space-between;
}

.shopListBottomL image {
    width: 30rpx;
    height: 30rpx;
    vertical-align: middle;
}

.shopListBottomL text {
    color: #9f9f9f;
    font-size: 24rpx;
    margin-right: 16rpx;
    margin-left: 4rpx;
}

