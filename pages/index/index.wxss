/* 弹框 */

page {
  height: 100%;
}

.dialog {
  position: fixed;
  top: 0;
  z-index: 9999;
  width: 65%;
  height: 100%;
  background-color: white;
  color: #655d6a;
  animation: myfirst 0.3s ease-out;
}

@keyframes myfirst {
  from {
    left: -65%;
  }

  to {
    left: 0;
  }
}

