

/* 搜索开始 */

.searchFa {
    width: 100%;
    box-sizing: border-box;
    padding: 0 20rpx;
    justify-content: space-between;
    display: flex;
    align-items: center;
    box-shadow: 0rpx 5rpx 10rpx rgb(199, 199, 199);
}

.search {
    padding: 10rpx;
    display: flex;
    align-items: center;
    width: 46%;
}

.search image {
    width: 35rpx;
    height: 30rpx;
}

.input {
    width: 300rpx;
    margin-left:10rpx;

}

.searchInp {
    display: flex;
    align-items: center;
    background: #f1f1f1;
    width: 100%;
    padding: 6rpx 30rpx;
    border-radius: 10rpx;
    box-sizing: border-box;
}

.sousuo {
    font-size: 28rpx;
}

.sousuoText {
    /* font-size: 34rpx; */
    color: white;
    background: #9ddfe5;
    padding: 5rpx 20rpx;
    border-radius: 10rpx;
}

.chooseYear {
    display: flex;
    align-items: center;
    font-size: 28rpx;
    color: #767676;
    line-height: 84rpx;
}

.chooseYear image {
    width: 30rpx;
    height: 25rpx;
    vertical-align: middle;
    margin-right: 20rpx;
    margin-left: 13rpx;
}

/* 搜索结束 */

/* 列表开始 */

.shopList {
    position: relative;
    margin: 64rpx;
    box-shadow: 0rpx 5rpx 30rpx rgb(146, 146, 146);
    position: relative;
    border-radius: 10rpx;
    /* background: #0f0; */
}

.shopListPart {
    width: 120%;
    height: 420rpx;
    position: absolute;
    top: 0rpx;
    left: -10%;
    box-shadow: 0rpx 6rpx 16rpx rgb(175, 175, 175);
    border-radius: 10rpx;
    overflow: hidden;
}

/* #myVideo{
  width: 100%;
  height: 420rpx;
} */

/* .shopListPart image {
  width: 100%;
  height: 100%;
} */

.shopListTitle {
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    background: rgba(0, 0, 0, 0.4);
    color: #fff;
    font-size: 24rpx;
    font-weight: 700;
    box-sizing: border-box;
    padding: 24rpx 20rpx;
    display: flex;
    justify-content: space-between;
    /* background: #69edef; */
}

/* .shopListTitle :nth-child(1) {
    width: 70%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
} */

.shopListTop {
    box-sizing: border-box;
    position: absolute;
    width: 100%;
    top: 0;
    left: -60rpx;
    padding: 10rpx 30rpx;
    display: flex;
    justify-content: space-between;
    z-index: 99;
}

.shopListTopL {
    color: #fff;
    width: 223rpx;
    margin-left: 20rpx;
    margin-top: 16rpx;
}

.year {
    font-size: 24rpx;
    letter-spacing: 40rpx;
}

.line {
    height: 1rpx;
    background: #969496;
    width: 85%;
}

.day {
    font-size: 38rpx;
    font-weight: 700;
    padding-right: 15%;
}

.shopListTopR {
    font-size: 32rpx;
    line-height: 80rpx;
}

.shopListTopR image {
    width: 40rpx;
    height: 40rpx;
    margin-right: 10rpx;
    vertical-align: middle;
}

.shopListBottom {
    padding: 30rpx 0;
    box-sizing: border-box;
    width: 90%;
    display: flex;
    justify-content: space-between;
    /* background: #00a2e9; */
    position: absolute;
    top: 420rpx;
    left: 5%;
    border-bottom: 1rpx solid #eaeaea;
}

.shopListBottomL image {
    width: 30rpx;
    height: 30rpx;
    vertical-align: middle;
}

.shopListBottomL text {
    color: #9f9f9f;
    font-size: 24rpx;
    margin-right: 16rpx;
    margin-left: 4rpx;
}

.xin {
    width: 32rpx;
    height: 32rpx;
}

.msgList {
    margin-top: 120rpx;
    box-sizing: border-box;
    padding: 30rpx;
    font-size: 22rpx;
    color: #7d7d7d;
}

/* 列表结束 */

/* 卡片列表结束 */

/* 底部导航栏颜色 */

.on_cor text {
    color: #00a2e9;
}

.on_cor icon {
    color: #00a2e9;
}

.video-item {
    position: relative;
    width: 100%;
    height: 420rpx;
}

video {
    width: 100%;
    height: 100%;
}

.video-cover {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
}

.video-play-btn {
    position: absolute;
    width: 60rpx;
    height: 60rpx;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    margin: auto;
}

.video-duration {
    position: absolute;
    right: 10px;
    bottom: 10px;
    color: #fff;
}

.videoimg {
    width: 100%;
    height: 420rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}
/* 直播 */
.live-player{
    width: 100%;
}
