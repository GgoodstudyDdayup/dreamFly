.time {
  margin: 0 5%;
  width: 90%;
  padding: 20rpx 0;
  border-bottom: 1rpx solid #eee;
}

.time-title {
  font-size: 26rpx;
  color: #473e3e;
  font-weight: 600;
  padding: 10rpx 20rpx;
}

.time text, .address text {
  padding: 0 20rpx;
  font-size: 24rpx;
  color: #888;
}

.address {
  width: 90%;
  padding: 20rpx 5%;
  border-bottom: 10rpx solid #f8f8f8;
}

.address-title {
  font-size: 26rpx;
  color: #473e3e;
  font-weight: 600;
  padding: 10rpx 20rpx;
}

.address-icon {
  float: right;
  padding-top: 30rpx;
  width: 40rpx;
  height: 45rpx;
}

.address-icon image {
  width: 100%;
  height: 100%;
}

/* 切换栏 */

.scroll {
  width: 100%;
  margin-top: 20rpx;
  display: flex;
}

.navTitleCon {
  width: 25%;
  display: inline-block;
  text-align: center;
  color: #473e3e;
  font-size: 24rpx;
}

.navTitleConTion .title {
  margin: 0 auto;
}

.navBorderTion {
  z-index: -1;
  margin: 0 auto;
  margin-top: 6rpx;
  width: 60rpx;
  border-bottom: 7rpx solid #ea4479;
}

/* 详情 */

.goods-title {
  color: #473e3e;
  font-size: 36rpx;
  font-weight: 600;
  margin-left: 5%;
  margin-top: 30rpx;
}

.goods-title text {
  color: #888;
  font-size: 22rpx;
  font-weight: 400;
}

/* 未展开的样式 */

.wr-bottom {
  height: 400rpx;
  line-height: 50rpx;
  overflow: hidden;
  padding: 20rpx;
}

/* 点击展开的样式 */

.openone {
  padding: 20rpx;
  height: auto;
  line-height: 50rpx;
}

/* 控制展开详情的样式 */

.open {
  height: 100rpx;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #ee8183;
  border-top: 1rpx solid #eee;
}

/* 底部按钮 */

.footer {
  position: fixed;
  bottom: 0;
  z-index: 9;
  box-sizing: border-box;
  width: 100%;
  height: 130rpx;
  padding: 30rpx;
  padding-left: 260rpx;
  background-color: white;
}

.weixin-icon {
  float: left;
  width: 60rpx;
  height: 50rpx;
  margin-left: -220rpx;
  margin-top: 15rpx;
}

.weixin-icon image {
  width: 92%;
  height: 95%;
}

.service-icon {
  float: left;
  width: 50rpx;
  height: 50rpx;
  margin-left: -100rpx;
  margin-top: 15rpx;
}

.service-icon image {
  width: 100%;
  height: 100%;
}

.pay {
  width: 100%;
  height: 80rpx;
  border-radius: 80rpx;
  background-color: #ee8183;
  line-height: 80rpx;
  font-size: 30rpx;
  color: white;
}

.pay::after {
  border: none;
}

/* 选择规格 */

.modal {
  position: fixed;
  bottom: 0;
  padding: 0 5%;
  width: 90%;
  padding-top: 20rpx;
  /* height: 700rpx; */
  background-color: white;
  border-radius: 20rpx 20rpx 0 0;
  transition: all 0.3s ease-out;
}

.modal1 {
  position: fixed;
  bottom: 0;
  padding: 0 5%;
  width: 90%;
  padding-top: 20rpx;
  height: 500rpx;
  background-color: white;
  border-radius: 20rpx 20rpx 0 0;
  transition: all 0.3s ease-out;
}

/* 未点击 */

.nomodal {
  margin-bottom: -1750rpx;
}

.dianjimodal {
  margin-bottom: 0;
}

.modal-title {
  color: #473e3e;
  font-size: 30rpx;
  font-weight: 600;
  padding: 20rpx 0;
}

.modal-title text {
  color: #888;
  font-size: 24rpx;
  margin-left: 10rpx;
  font-weight: 400;
}

.priceView {
  display: flex;
  flex-direction: column;
}

.select {
  display: inline-block;
  padding: 10rpx 30rpx;
  border-radius: 10rpx;
  margin: 0 10rpx;
}

/* 未选择 */

.notselecting {
  margin-top: 4px;
  color: #888;
  background-color: #eee;
}

/* 已选择 */

.selecting {
  margin-top: 4px;
  color: white;
  background-color: #ee8183;
}

/* 二维码 */

.erweima {
  width: 100%;
  line-height: 60rpx;
  text-align: center;
}

.erweima view {
  color: #888;
  font-size: 26rpx;
  margin: 0 auto;
}

.erweima image {
  width: 250rpx;
  height: 250rpx;
  margin-bottom: 100rpx;
}

.price {
  display: inline-block;
}

.money-input {
  height: 38rpx;
  display: inline-block;
}

.money-input input {
  display: inline-block;
  width: 100rpx;
  border: 1rpx solid #eee;
  border-radius: 15rpx;
  text-align: center;
  /* vertical-align: center; */
}

.money-input image {
  height: 38rpx;
  width: 38rpx;
  margin: 0 10rpx 8rpx 10rpx;
}

.buttontwo {
  width: 100%;
  height: 80rpx;
  margin: 30rpx 0;
}

.buttontwo button {
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 80rpx;
}

.buttontwo button::after {
  border: none;
}

.buttontwo-l {
  float: left;
  width: 38%;
  border: 1rpx solid #ee8183;
  color: #ee8183;
  background-color: white;
}

.buttontwo-r {
  float: right;
  width: 100%;
  color: white;
  background-color: #ee8183;
}

/* 遮盖 */

.cover {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 6;
  width: 100%;
  height: 100%;
  background-color: #000;
  opacity: 0.6;
  cursor: default;
}

.video {
  margin-top: 30rpx;
  width: 90%;
  padding: 0 5%;
}

.video-item {
  height: 250rpx;
  margin-bottom: 30rpx;
  display: flex;
  justify-content: flex-start;
}

.item-img {
  width: 30%;
  height: 100%;
}

.video-itemView {
  width: 100%;
  height: 250rpx;
  margin-bottom: 30rpx;
  display: flex;
  justify-content: flex-start;
}

.item-img image {
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}

.item-info {
  width: 68%;
  height: 100%;
  padding-left: 2%;
  display: flex;
  flex-direction: column;
}

.info-title {
  color: #473e3e;
  font-weight: 600;
  font-size: 30rpx;
  height: 80rpx;
}

.info-name, .info-num, .info-price {
  margin-top: 20rpx;
  color: #888;
  font-size: 24rpx;
}

.info-price {
  color: #ee8183;
}
