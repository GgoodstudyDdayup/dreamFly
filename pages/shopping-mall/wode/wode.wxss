.mask {
  position: fixed;
  top: 0rpx;
  left: 0rpx;
  z-index: 998;
  width: 100%;
  height: 100%;
  background: #666;
  opacity: 0.6;
}

.mypic {
  position: fixed;
  top: 260rpx;
  background: whitesmoke;
  width: 60%;
  left: 20%;
  height: 350rpx;
  border-radius: 20rpx;
  z-index: 999;
  text-align: center;
}

.mypic button {
  width: 90%;
  margin: 30rpx auto;
}
page {
    padding-bottom: 0;
}

.window {
    width: 92%;
    margin: 0 auto;
}

/* 通用字体 */

.fonts {
    font-weight: 900;
}

.txt1 {
    font-size: 28rpx;
    margin:15rpx 0;
}

.js-bg {
    width: 50rpx;
    height: 50rpx;
    background: #ededed;
    border-radius: 9rpx;
    margin: 0 8rpx;
    line-height: 50rpx;
    text-align: center;
    font-weight: 200;
}

.ones {
    width: 100%;
    height: 125rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.jishi {
    display: flex;
    align-items: center;
}

.twos {
    display: flex;
    justify-content: space-between;
}

.two1 {
    font-size: 20rpx;
    color: #999;
}

.twos1 {
    margin: 30rpx 0;
    /* border: 1rpx solid #000; *//* background: #999999; */
    display: flex;
    align-items: center;
}

.piao {
    width: 180rpx;
    height: 50rpx;
    line-height: 50rpx;
    text-align: center;
}

.piao1 {
    background: #5bc9d4;
    color: #fff;
    margin: 0 15rpx;
}

.piao2 {
    background: #f4f0f5;
}

.btn {
    height: 230rpx;
    display: flex;
    align-items: center;
    /* background: #999999; */
}

.kanbtn, .maibtn {
    height: 120rpx;
    line-height: 120rpx;
    text-align: center;
    font-size: 34rpx;
    border-radius: 9rpx;
    color: #fff;
}

.kanbtn {
    width: 420rpx;
    margin: 0 20rpx 0 25rpx;
}

.maibtn {
    width: 200rpx;
    background: #333;
}

/*砍价记录开始 */

.windows {
    background: #fff;
    margin-top: 15rpx;
}

.flex {
    width: 92%;
    margin: 0 auto;
    display: flex;
}

.thr {
    justify-content: space-between;
    line-height: 75rpx;
}

.thr1 {
    justify-content: space-around;
}

.jilu {
    /* background: #0f0; */
    display: flex;
    justify-content: space-between;
	padding: 10rpx 30rpx;
	align-items: center;
}
.jilu_f{
	display: flex;
	justify-content: space-between;
	align-items: center;
}
.kan-name{
    line-height: 54rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
/*砍价记录结束 */

/* 进度条部分开始 */

.jindutiao {
    width: 90%;
    height: 115rpx;
    position: relative;
    margin: auto;
}

.quan {
    width: 20rpx;
    height: 20rpx;
    border-radius: 50%;
    background: #ededed;
    position: absolute;
    top: 53rpx;
}

.weizhi {
    width: 100%;
    height: 30rpx;
    position: absolute;
    top: 56rpx;
}

.qipao {
    width: 147rpx;
    height: 43rpx;
    font-size: 20rpx;
    color: #fff;
    text-align: center;
    line-height: 36rpx;
    /* border-radius: 9rpx; */
    background-size: 100%;
}

.dian text {
    position: absolute;
    top: 76rpx;
}

.lan {
    background: #5bc9d4;
}

/* 进度条部分结束 */
/* 遮罩 */
.zhezhao{
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 99;
    display:flex;
    align-items:center;
    background-color: #fff; 
}
.shouquan{
    width:300rpx;
    background:#1AAD19;
    color:#fff;

}
.cutButton {
  margin: 0 5%;
  width: 90%;
  height: 100rpx;
  padding: 30rpx 0;
  border-bottom: 1rpx solid #eee;
}
.cutButton button {
  line-height: 80rpx;
  margin: 10rpx 5%;
}
.cutButton button::after {
  border: none;
}
.helpCut {
  float: left;
  width: 320rpx;
  height: 80rpx;
  border-radius: 80rpx;
  border: none;
  background-color: #ee8183 ;
  color: white;
}
.buy {
  float: left;
  width: 220rpx;
  height: 80rpx;
  border-radius: 80rpx;
  border: 1rpx solid #ee8183 ;
  color: #ee8183 ;
  background-color: white;
}

.help-record {
  margin-left: 5%;
  padding: 10rpx;
  width: 150rpx;
  border-bottom: 4rpx solid #fec227;
  color: #4f4747;
  font-size: 36rpx;
  font-weight: 600;
}