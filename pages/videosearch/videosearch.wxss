.headSearch-search {
  position: relative;
  /* width: 73%; */
  width: 100%;
  height: 90rpx;
  /* padding: 0 5%; */
}

.headSearch-search .headSearch-search-icon {
  position: absolute;
  left: 30rpx;
  top: 50%;
  width: 30rpx;
  height: 30rpx;
  margin-top: -47rpx;
}

.headSearch-search .headSearch-search-icon image {
  width: 30rpx;
  height: 30rpx;
}

.headSearch-search input {
  box-sizing: border-box;
  width: 88%;
  height: 58rpx;
  border: 2rpx solid #9b969f;
  border-radius: 60rpx;
  margin-top: 14rpx;
  padding-left: 70rpx;
}

/* 切换栏 */

.tabselect {
  padding: 0 5%;
  width: 90%;
  position: absolute;
  left: 480rpx;
  top: 10rpx;
}

.tabselect-item {
  text-align: center;
  color: #473e3e;
  font-size: 26rpx;
  display: flex;
  flex-direction: row;
}

.tabselect-item view, .tabselect-item picker {
  width: 50%;
  height: 70rpx;
  line-height: 70rpx;
}

.tabselect-item image {
  width: 20rpx;
  height: 10rpx;
  margin-left: 10rpx;
  vertical-align: middle;
}
