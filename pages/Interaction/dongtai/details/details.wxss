/* 留言 */
.liuyan-top{
  padding: 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid #e6e6e6;
}
.liuyan-top text:nth-child(1){
  font-size: 45rpx;
  color: black;
}
.liuyan-item{
  padding: 30rpx;
}
.liuyanItem-top image{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}
.liuyanItem-top{
  display: flex;

}
.liuyanItemTopRright{
  margin-left: 10rpx;
}
.liuyanItemTopRright view:nth-child(2){
  font-size:35rpx;
  color:black;
  margin-top:10rpx;
}
.liuyanTime{
  display: flex;
  justify-content:space-between; 
}
.liuyanTime >text{
  margin-left: 107rpx;
}
.liuyanTime .icon{
  margin-left: 17rpx;
}
.content image{
  width: 100%;
