/* pages/Interaction/Interaction.wxss *//* 横向滚动开始 */

.scroll {
    background: #fff;
    overflow: hidden;
    width: 100%;
    white-space: nowrap;
    display: flex;
    border-bottom: 1rpx solid #eaeaea;
    justify-content: space-between;
    margin-bottom: 16rpx;
}

.navTitle {
    width: 33%;
    color: #282828;
    font-size: 26rpx;
    display: inline-block;
    height: 50rpx;
    line-height: 50rpx;
    text-align: center;
    padding: 16rpx 0;
}

.navTitleLine {
    width: 33%;
    color: #5bc9d4;
    font-size: 30rpx;
    display: inline-block;
    height: 55rpx;
    line-height: 50rpx;
    text-align: center;
    padding: 10rpx 0;
    border-bottom: 5rpx solid #5bc9d4;
}

.navTitleCon {
    padding: 0 60rpx;
    height: 50rpx;
    line-height: 50rpx;
    text-align: center;
    border-left: 1rpx solid #f1f1f1;
    /* border-right: 1rpx solid #f1f1f1; */
}

/* 横向滚动结束 */