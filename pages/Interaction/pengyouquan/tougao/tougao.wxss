/* pages/personalCenter/tougao/tougao.wxss */
page{
  background: #f3f3f3;
}
.header{
  height: 100rpx;
  background: #f3f3f3;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding:0 28rpx;
}
.header-left{
  font-size: 30rpx;
  color: black;
}
.header-right{
  background: #fff;
  padding: 10rpx;
}
/* 头部结束 */
.biaoti{
  height: 140rpx;
  background: #fff;
  padding: 30rpx;
  box-sizing: border-box;
}
.biaoti input{
  width: 100%;
  height: 100%;
  background: #fafafa;
  padding-left: 20rpx;
  box-sizing: border-box;
}
.neirong{
  height: 326rpx;
  background: #fff;
  padding: 30rpx;
  box-sizing: border-box;
}
.neirong textarea{
  width: 100%;
  height: 100%;
  background: #fafafa;
  padding-left: 20rpx;
  box-sizing: border-box;
  padding: 20rpx;
}

.duomeiti-item{
  background: #fff;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 30rpx;
  border-bottom:1px solid #f3f3f3; 
}
.duomeiti-item-left{
  font-size: 30rpx;
  color: black;
}
.duomeiti-item-left icon{
  font-size: 38rpx;
  color: black;
  margin-right: 10rpx;
}
.submit{
  height: 100rpx;
  margin:0 25rpx;
  text-align: center;
  line-height: 100rpx;
  color: #fff;
  background-size:100% 100%; 
  margin-top: 50rpx;
}
.yulanimg{
  padding:25rpx;
}
.yulanimg image{
  width: 250rpx;
  height: 250rpx;
}
.piker{
  width:500rpx;
}
.biaoti1{
  height: auto;
}
