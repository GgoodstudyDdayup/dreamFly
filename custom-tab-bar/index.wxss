.tab-bar {
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    background: white;
    display: flex;
    padding: 20rpx 0;
}

.tab-bar-border {
    background-color: rgba(0, 0, 0, 0.33);
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 1px;
    transform: scaleY(0.5);
}

.tab-bar-item {
    flex: 1;
    text-align: center;
    position: relative;
}

.tab-bar-item image {
    width: 27px;
    height: 27px;
    display:block;
    margin:auto;
}
.tab-bar-item view {
    font-size: 10px;
}
